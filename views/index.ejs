<!DOCTYPE html>
<html>
<head>
    <title>My Dev Skills</title>
</head>
<body>

    
    <h1>Welcome to My Dev Skills</h1>

    <ul>
        <% skills.forEach(function(skill) { %>
            <li><%= skill.name %>: <%= skill.description %></li>
        <% }); %>
    </ul>

   
    <a href="/skills/new">Add Skill</a>


</body>
</html>